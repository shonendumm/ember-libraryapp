
<h1>Messages from Users (who contacted us)</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Message</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |contact|}}
    <tr>
      <th>{{contact.email}}</th>
      <td>{{contact.message}}</td>
      <td><button class="btn btn-danger btn-xs" {{action 'deleteMessage' contact}}>Delete</button></td>
    </tr>
  {{/each}}
  </tbody>
</table>
